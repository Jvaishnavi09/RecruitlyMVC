<div class="d-flex mt-5 justify-content-center gap-2">
  <% if(applicants.length===0 ){ %>
  <p>No Applicants for the job yet...</p>
  <%}%> <% applicants.forEach( applicant => { %>
  <div class="card" style="width: 18rem">
    <div class="card-body">
      <h5 class="card-title"><%= applicant.name %></h5>
      <p class="card-text"><%= applicant.email %></p>
      Contact Number : <%= applicant.contact %>
      <div class="d-flex justify-content-center mt-2">
        <Button
          class="btn btn-primary"
          onclick="handleResume('<%=applicant.resumePath%>')"
          >RESUME
        </Button>
      </div>
    </div>
  </div>
  <% })%>
</div>
